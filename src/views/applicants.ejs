<div class="aplnt_lt">
    <h1 class="aplnt_hd">Easily - Applicants</h1>
</div>

<table class="table table-striped">
    <thead class="table-dark">
      <tr>
        <th scope="col">Index</th>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Company Name</th>
        <th scope="col">Position</th>
        <th scope="col">Phone Number</th>
        <th scope="col">Resume</th>
      </tr>
    </thead>
    <tbody>
        <% jobApplicants.forEach(jobApplicant => { %>
            <tr>
                <th scope="row"><%= jobApplicant.index %></th>
                <td><%= jobApplicant.name %></td>
                <td><%= jobApplicant.email %></td>
                <td><%= jobApplicant.companyName %></td>
                <td><%= jobApplicant.position %></td>
                <td><%= jobApplicant.phoneNo %></td>
                <td><a href="<%= jobApplicant.resume %>" target="_blank"><i class="fa-solid fa-file"></i></a></td>
              </tr>
        <% }) %>
    </tbody>
  </table>